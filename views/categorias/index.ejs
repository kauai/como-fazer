<%- include('../header')%>
<div class="table-responsive-sm">
<table class="table table-hover table-dark table-striped">
    <thead>
        <tr>
            <th scope="col">Categoria</th>
            <th scope="col">Editar</th>
            <th scope="col">Excluir</th>
        </tr>
      </thead>
  <tbody>
 <% categorias.forEach(({ id, categoria }) => { %>
        <tr>
            <td><a href="/publicacoes/categoria/<%= id %>"><%= categoria %></a></td>
            <td><a class="btn btn-sm btn-primary" href="/categorias/editar/<%= id %>">Editar</a> </td>
            <td><a class="btn btn-sm btn-danger" href="/categorias/excluir/<%= id %>">Excluir</a></td>
        </tr>
 <% }) %>
</tbody>
</table>
</div>
<%- include('../footer')%> 


 
  
